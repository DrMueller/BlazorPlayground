@using BlazorBootstrap

<CascadingValue Value="@(this)" IsFixed="true">
    @ChildContent
</CascadingValue>

<Tabs>
    @foreach (var lt in LazyTabsList)
    {
        <Tab Title="Test" Active="true">
            <Content>
                @lt.ChildContent
            </Content>
        </Tab>
    }
</Tabs>